{
    "name": "iatstuti/laravel-nullable-fields",
    "description": "This trait allows you to easily flag attributes that should be set as null when being persisted to the database using the Laravel PHP Framework.",
    "type": "utility",
    "require": {
        "php": ">=5.4",
        "illuminate/database": "~5.0",
        "illuminate/events": "~5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au",
            "homepage": "https://dyrynda.com.au"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Iatstuti\\Database\\Support\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
